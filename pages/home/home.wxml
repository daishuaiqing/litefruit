<!--pages/home/home.wxml-->
<view class="container-box">
  <!--首页轮播-->
  <swiper class="banner-box" indicator-dots="true" autoplay="true" interval="5000" indicator-active-color="#48b871" circular="true" duration="500">
    <block wx:for="{{imgUrls}}" wx:key="index">
      <swiper-item>
        <image src="{{item}}" class="slide-image" width="100%" />
      </swiper-item>
    </block>
  </swiper>
</view>
